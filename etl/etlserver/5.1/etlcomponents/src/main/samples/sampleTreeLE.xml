				</row>
				<row>
					<value>1</value>
					<value>Components</value>
					<value>1</value>
					<value>C</value>
					<value>CO</value>
				</row>
				<row>
					<value>2</value>
					<value>Handlebars</value>
					<value>1</value>
					<value>N</value>
					<value>HB</value>
				</row>
				<row>
