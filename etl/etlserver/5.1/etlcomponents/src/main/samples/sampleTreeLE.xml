				</row>
				<row>
					<value>1</value>
					<value>Clothing</value>
					<value>1</value>
					<value>C</value>
					<value>CL</value>
				</row>
				<row>
					<value>2</value>
					<value>Bib-Shorts</value>
					<value>1</value>
					<value>N</value>
