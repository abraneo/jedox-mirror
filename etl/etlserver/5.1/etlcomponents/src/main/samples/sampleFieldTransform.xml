      <connection nameref="Out_Mapping" />
    </load>
    <load name="L_OlapData" type="File" modified="1383649194228" modifiedBy="">
      <source nameref="T_OlapData" />
      <connection nameref="Out_OlapData" />
    </load>
  </loads>
  <jobs>
    <job name="default" type="Standard" modified="1383649194228" modifiedBy="">
      <execution type="load" nameref="L_Text" />
      <execution type="load" nameref="L_Mapping" />
      <execution type="load" nameref="L_Date" />
      <execution type="load" nameref="L_Row" />
      <execution type="load" nameref="L_OlapData" />
    </job>
  </jobs>
