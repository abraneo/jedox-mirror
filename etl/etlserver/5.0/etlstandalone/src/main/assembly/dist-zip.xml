<assembly>
  <id>bin</id>
  <formats>
    <format>zip</format>
    <format>dir</format>
  </formats>
  <includeBaseDirectory>true</includeBaseDirectory>
  <fileSets>
    <fileSet>
      <directory>src/main/config</directory>
      <outputDirectory>config</outputDirectory>
      <fileMode>0644</fileMode> <!-- 420 -->
      <directoryMode>0755</directoryMode>
    </fileSet>
 <!-- 
    <fileSet>
      <directory>src/main/projects</directory>
      <outputDirectory>projects</outputDirectory>
      <fileMode>0644</fileMode>
      <directoryMode>0755</directoryMode>
    </fileSet>
 -->    
    <fileSet>
      <directory>src/main/db</directory>
      <outputDirectory>data/db</outputDirectory>
      <fileMode>0644</fileMode> <!-- 420 -->
      <directoryMode>0755</directoryMode>
    </fileSet>
    <fileSet>
      <directory>src/main/customlib</directory>
      <outputDirectory>customlib</outputDirectory>
      <fileMode>0644</fileMode> <!-- 420 -->
      <directoryMode>0755</directoryMode>
    </fileSet>
    <fileSet>
      <directory>src/main/bin</directory>
      <outputDirectory/>
      <fileMode>0755</fileMode>
      <directoryMode>0755</directoryMode>
     </fileSet>
  </fileSets>
<!--   
  <files>
    <file>
	  <source>COPYING.txt</source>
	  <outputDirectory/>
	  <fileMode>0644</fileMode>
	</file>
	<file>
	  <source>README.txt</source>
	  <outputDirectory/>
	  <fileMode>0644</fileMode>
	  <filtered>true</filtered>
	</file>
  </files>
 -->  
  <dependencySets>
    <dependencySet>
      <outputDirectory>lib</outputDirectory>
      <scope>runtime</scope>
      <excludes>
        <exclude>${groupId}:${artifactId}</exclude>
        <exclude>${groupId}:etlpremium</exclude>
      </excludes>
    </dependencySet>
    <dependencySet>
      <outputDirectory>config</outputDirectory>
      <unpack>true</unpack>
      <scope>provided</scope>
      <includes>
        <include>${groupId}:etlcomponents:zip:config</include>
      </includes>
    </dependencySet>
     <dependencySet>
      <outputDirectory></outputDirectory>
      <unpack>true</unpack>
      <scope>provided</scope>
      <includes>
        <include>${groupId}:etlcomponents:zip:samples</include>
      </includes>
    </dependencySet>
  </dependencySets>
</assembly>
